.accordion .card-header{background:#fff;padding:0;border:0;}
.accordion .dropdownToggle{position:relative;font-size: 12px;line-height: 14px;padding:0 18px;}
.accordion .dropdownToggle[aria-expanded="true"]:after{content: "-";position: absolute;left: 0;top: 0;font-size: 20px;color: #7F7F7F;}
.accordion .dropdownToggle[aria-expanded="false"]:after{content: "+";position: absolute;left: 0;top: 0;color: #7F7F7F;font-size: 18px;}
.accordion .card-body{font-size: 12px;line-height: 16px;}
.colorSwatch{width:12px;height:12px;display:inline-block;border-radius:50%;margin:0 3px;position:relative;}
.colorSwatch:hover{border:0 !important;}
.addToCart{width: 100%;background: #000 !important;border: 0 !important;height: 50px;margin: 20px 0 !important;}
.swatch .header{display:inline-block;}
.swatch .swatch-element{float:right !important;margin:10px 10px 10px 0;}
.swatch[data-option-index="0"] label{min-width: 20px !important;height: 20px !important;border-radius: 50%;position:relative;}
.swatch[data-option-index="0"] .tooltip{width:50px;background: #F4F4F4;color: #8b8d8b;text-transform:capitalize;left:-15px;padding:5px;}
.swatch[data-option-index="0"] .tooltip:after{border-top: solid #F4F4F4 10px;}
.swatch[data-option-index="1"] label{background:transparent !important;border:none !important;}

/* ----------------responsive---------------- */



@media (max-width: 1440px){
  .yaya_newsletter .product_items{ width: 100%;}
  .social {padding-left: 3px;}
  .footer_menu .col-md-2{padding: 0;}
  .footer_menu a{font-size: 10px;}
}

@media (max-width: 1366px){
  .footer_sec .col-md-2 , .footer_sec .col-md-6{padding: 0;}
}

@media (max-width: 1199px){
  .menu_bar li,.head_section .head_right li{padding: 0 10px;}
  .newsletter input::placeholder {font-size: 12px;}
  .newsletter p{font-size: 14px;}
  .newsletter_des{max-width: 100%;}
  .newsletter_des p{font-size: 26px;padding-bottom: 60px;}
}

@media (max-width: 1024px){
  .get_look_des a{padding: 10px 20px;}
  .get_look_des h3{font-size: 30px;line-height: 30px;}
  .description-top-right{right: -10%;left: 64%;}
  .description-bottom-left {bottom: 20%;left: 30%;}
  .shop_collection h3 {font-size: 25px;}
  .shop_collection p {font-size: 12px;}
  .shop_arrival_heading p{padding-top: 20px;}
  .sale h2 {font-size: 130px;line-height: 150px;}
  .sale p {font-size: 70px;line-height: 85px;}
}
@media (max-width: 991px){
  .banner_text{top: 30%;}
  .banner_text h2 {font-size: 50px;line-height: 60px;}
  .banner_text a {margin-top: 25px;padding: 12px 50px;font-size: 12px;}
  .shop_arrival_heading{padding: 50px 0;}
  .get_look_des h3 {    font-size: 20px;line-height: 20px;padding-bottom: 10px !important;}
  .get_look_des a {font-size: 10px;padding: 8px 15px;}
  .shop_coll_slider, .shop_back{padding-bottom: 50px;}
  .slick-prev, .slick-next{top: 30%;  }
  .newsletter_des p {font-size: 20px;padding-bottom: 30px;}
  .footer_sec .about{order: 1;}
  .footer_sec .footer_menu{order: 3;}
  .footer_sec .newsletter{order: 2;}
  .footer_sec .col-sm-6{padding:0 15px 30px;}
  .footer .footer_sec{padding-top: 0;}
  .footer h4{padding: 25px 0 10px;}
  .menu_bar li, .head_section .head_right li {padding: 0 6px;}
  .head_section .col-md-4{padding: 0;}
}
@media (max-width: 767px){
  .head_section .col-md-4 {width: auto;}
  .menu_left_mb,.menu_right_mb{align-items: center;}
  .menu_bar{margin-left:0;}
  .head_right{margin-right:0;}
  .video-control__play-wrapper{display:block;}
  .shop_arrival_heading h2{font-size: 20px;line-height: 20px;}
  .sale h2{font-size: 100px;line-height: 100px;}
  .sale p{font-size: 60px;line-height: 70px;}
  .newsletter_des p {font-size: 15px;line-height: 30px;}
  .shop_collection h3{font-size: 20px;line-height: 25px;padding: 10px 0;}
  .banner_text h2 {font-size: 40px;line-height: 45px;}
  .banner_text a {margin-top: 10px;padding: 10px 40px;font-size: 12px;}
}