.head_section .col-md-4{z-index: 3;align-items: center;justify-content: center;display: flex;}
.head_section{position: relative;}
.menu_bar{position: initial!important;}
.head_section .row{padding: 15px;margin: 0;}
.child_menu{display: none;}
.mega_menu:hover > .child_menu{  display:inline-block;position: absolute;z-index: 3;}
.child_menu .col-lg-6{column-count:3;margin-top:32px;}
