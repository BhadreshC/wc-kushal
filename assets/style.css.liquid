.login_item{width: 550px;height: 100%;z-index: 4;position: fixed;top: 0;right: 0;max-width: 100%;background: #fff;transform: translateX(100%);transition: 1000ms;padding: 14px 25px;display: flex!important;align-items: center;}
.login_close svg{height: 14px;width: 14px;fill: #000;}
button.login_close{border: 0;background: transparent;cursor: pointer;flex: 1;}
.grid{margin-left: 0;}
.medium-up--push-one-quarter{left: 0;padding: 0;}
#customer_login label{font-size: 13px;line-height: 20px;font-weight: 300;}
#CustomerEmail:focus,#CustomerPassword:focus{border: 1px solid #000 !important;}
#CustomerEmail, #CustomerPassword{padding: 12px 18px;margin-bottom: 28px;}
.fb svg,.google svg{width: 20px;height: 20px;margin: 0 15px -5px 0;}
.btn.login{font-size: 13px;line-height: 16px;width: 100%;border-radius: 0;background: #000;padding: 16px;margin-bottom: 12px;}
.google{color: #8E8E8E;font-size: 13px;line-height: 20px;border: 1px solid #000;padding: 14px;margin-bottom: 12px;}
.fb{font-size: 13px;line-height: 20px;color: #fff;background: #3B5898;padding: 15px;}
.fb:hover{color: #fff !important;}
#RecoverPassword{font-size: 10px;line-height: 20px;border-bottom: 1px solid;padding-bottom: 5px;}
#LoginHeading{font-size: 25px;line-height: 30px;flex: 1;}
.login_top{margin-bottom: 27px;}
#customer_register_link,.log_in,#HideRecoverPasswordLink{font-size: 13px;line-height: 16px;border: 1px solid #000;padding: 14px;color: #000;display: block;}
.member_yet{font-size: 13px;line-height: 20px;margin-bottom: 9px;}
.login_bottom{margin-top: 70px;}





/* ----------------register drawer---------------- */




.active_to_register{position: relative;overflow: hidden;}
.active_to_register .register_item {transform: translateX(0px);}
.active_to_register .register_overly{visibility: visible;opacity: 0.5;}
