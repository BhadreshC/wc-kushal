.search_items{position: absolute;display: none;background: #fff;}
.search_items div{padding: 12px;margin: 12px;border-bottom: 1px solid;}
.search_arrow{position: absolute;float: right;border: 0;background: transparent;}
.search_arrow:focus{outline: 0;}
.search_svg{position: absolute;}
.bag_svg{width: 20px;}
.menu_img figure{position: relative;}
