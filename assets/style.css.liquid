.text_bottom{top: 85%;}
.text_top{top: 14%;}


/* ----------------login drawer---------------- */


.active_to_login{position: relative;overflow: hidden;}
.active_to_login .login_item {transform: translateX(0px);}
.active_to_login .login_overly{visibility: visible;opacity: 0.5;}
