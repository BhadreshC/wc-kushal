*{margin: 0 auto;list-style: none;text-decoration: none;font-weight: 400;font-family: 'Nimbus Sans L';}
dl,ul,ol,p{margin-top: 0 !important;margin-bottom: 0 !important;}
h1, h2, h3, h4, h5, h6{margin-bottom: 0 !important;}
